<fop version="1.0">
	<renderers>
	 	<renderer mime="application/pdf">
			<filterList>
				<!-- provides compression using zlib flate (default is on) -->
				<value>flate</value>
			</filterList>
			<fonts>
				<font metrics-url="Fonts/arial.xml" embed-url="../../../../../../Fonts/ARIALUNI.TTF"> 
					<font-triplet name="Arial" style="normal" weight="normal"/>
				</font>
				<!-- <font metrics-url="Fonts/fsb.xml" embed-url="Fonts/FreeSerifBold.ttf"> 
					<font-triplet name="FreeSerif" style="normal" weight="bold"/>
				</font> -->
				<!-- <auto-detect /> -->
			</fonts>
		</renderer>
	</renderers>
</fop>
